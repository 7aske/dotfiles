{
    "arduino.defaultBaudRate": 9600,
    "arduino.path": "/opt/arduino-1.8.10",
    "breadcrumbs.enabled": true,
    "editor.cursorBlinking": "solid",
    "editor.cursorStyle": "block",
    "editor.fontFamily": "Fira Code Medium",
    "editor.fontLigatures": true,
    "editor.fontSize": 16,
    "editor.fontWeight": "400",
    "editor.lineNumbers": "relative",
    "editor.minimap.enabled": false,
    "editor.multiCursorModifier": "ctrlCmd",
    "editor.suggestSelection": "first",
    "editor.tabSize": 4,
    "explorer.confirmDelete": false,
    "extensions.autoUpdate": true,
    "problems.decorations.enabled": false,
    "extensions.experimental.affinity": {
        "vscodevim.vim": 1
    },
    "files.autoSave": "onFocusChange",
    "files.exclude": {
        "**/.git": true,
        "**/.svn": true,
        "**/.hg": true,
        "**/CVS": true,
        "**/.DS_Store": true,
        "**/node_modules": true,
    },
    "github.copilot.nextEditSuggestions.enabled": true,
    "liveSassCompile.settings.generateMap": false,
    "search.mode": "newEditor",
    "shellcheck.customArgs": ["-x"],
    "terminal.integrated.allowChords": false,
    "terminal.integrated.cursorBlinking": false,
    "terminal.integrated.fontFamily": "Fira Code",
    "terminal.integrated.fontSize": 14,
    "terminal.integrated.shell.windows": "bash.exe",
    "vim.cursorStylePerMode.insert": "line",
    "vim.enableNeovim": true,
    "vim.highlightedyank.color": "#A3BE8C",
    "vim.highlightedyank.enable": true,
    "vim.hlsearch": true,
    "vim.incsearch": true,
    "vim.leader": " ",
    "vim.smartcase": true,
    "vim.surround": true,
    "vim.useCtrlKeys": true,
    "vim.useSystemClipboard": true,
    "vim.vimrc.enable": true,
    "vim.vimrc.path": "/home/nik/.config/Code/User/init.vim",
    "vim.handleKeys": {
        "<C-r>": true,
        "<C-w>": true,
        "<C-p>": false,
    },
    "vim.insertModeKeyBindings": [
        {
            "before": ["<leader>", "e"],
            "commands": ["workbench.view.explorer"]
        },
        {
            "before": ["<leader>", "<leader>"],
            "commands": ["workbench.action.showCommands"]
        }
    ],
    "vim.normalModeKeyBindingsNonRecursive": [
        {
            "before": ["<leader>", "e"],
            "commands": ["workbench.view.explorer"]
        },
        {
            "before": ["<leader>", "<leader>"],
            "commands": ["workbench.action.showCommands"]
        },
        {
            "before": ["C-w", "q"],
            "commands": ["workbench.action.closeActiveEditor"]
        }
    ],
    "window.openFilesInNewWindow": "off",
    "window.title": "${dirty}${rootName}${separator}${activeEditorShort}",
    "window.titleBarStyle": "custom",
    "window.zoomLevel": 0,
    "workbench.colorTheme": "Nord",
    "workbench.iconTheme": "material-icon-theme",
    "workbench.sideBar.location": "left",
    "workbench.startupEditor": "none",
    "workbench.colorCustomizations": {
        "statusBar.noFolderBackground": "#343434",
        "statusBar.background": "#232323",
    },
}
